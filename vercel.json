{
  "version": 2,
  "buildCommand": "cd pmc-frontend && npm install && npm run build",
  "outputDirectory": "pmc-frontend/build",
  "devCommand": "cd pmc-frontend && npm start",
  "installCommand": "cd pmc-frontend && npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}